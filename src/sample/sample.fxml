               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button fx:id="autoProcessorsButton" mnemonicParsing="false" onAction="#mikProcessorsAction" text="Autofarmer" visible="false" GridPane.rowIndex="9" />
