            <Label fx:id="priceFarmereLabel" text="0" textAlignment="RIGHT" GridPane.columnIndex="6" GridPane.rowIndex="4">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label text="Antal Mik Farmere:" GridPane.columnIndex="5" GridPane.rowIndex="3" />
            <Label fx:id="antalFarmereLabel" text="0" textAlignment="RIGHT" GridPane.columnIndex="6" GridPane.rowIndex="3">
