                      <Insets left="100.0" />
                  </GridPane.margin>
                  <font>
                      <Font size="26.0" />
                  </font>
              </Label>
              <Button fx:id="autoclipperButton" mnemonicParsing="false" onAction="#autoclipperButtonAction" text="Autoclipper" visible="false" GridPane.rowIndex="5">
