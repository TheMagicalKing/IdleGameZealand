          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
              <Button fx:id="makeClipButton" mnemonicParsing="false" onAction="#makeMikButtonAction" text="Makeclip" GridPane.columnIndex="3" GridPane.rowIndex="2" />
              <Label text="Paperclips" GridPane.columnIndex="3">
                  <font>
                      <Font size="52.0" />
                  </font>
              </Label>
              <Label text="Antal miks:" GridPane.columnIndex="3" GridPane.rowIndex="1" />
              <Label fx:id="antalClipsLabel" text="0" textAlignment="RIGHT" GridPane.columnIndex="3" GridPane.rowIndex="1">
